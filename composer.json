{
    "name": "tempest-tools/gedmo-doctrine-extensions-wrapper",
    "type": "library",
    "description": "Tempest Tools Gedmo Doctrine extension wrapper",
    "keywords": [
        "behaviors",
        "doctrine2",
        "extensions",
		"tempest-tools",
        "gedmo",
		"gedmo-doctrine-extensions-wrapper",
        "sluggable",
        "loggable",
        "translatable",
        "tree",
        "nestedset",
        "sortable",
        "timestampable",
        "blameable",
        "uploadable"
    ],
    "homepage": "http://gediminasm.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Jerome Erazo",
            "email": "jerome@tempest.house"
        },
        {
            "name": "Will Ferrer",
            "email": "will@tempest.house"
        },
        {
            "name": "Tempets House",
            "email": "info@tempest.house"
        }
    ],
    "support": {
        "email": "info@tempest.house"
    },
    "require": {
        "php": ">=5.3.2",
        "behat/transliterator": "~1.2",
        "doctrine/common": "~2.4"
    },
    "require-dev": {
        "doctrine/mongodb-odm": ">=1.0.2",
        "doctrine/orm": ">=2.5.0",
        "doctrine/common": ">=2.5.0",
        "symfony/yaml": "~2.6|~3.0|~4.0",
        "phpunit/phpunit": "^4.8|^5.7|^6.5"
    },
    "suggest": {
        "doctrine/mongodb-odm": "to use the extensions with the MongoDB ODM",
        "doctrine/orm": "to use the extensions with the ORM"
    },
    "autoload": {
        "psr-4": { "Gedmo\\": "lib/Gedmo" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4.x-dev"
        }
    }
}
